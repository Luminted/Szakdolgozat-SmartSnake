{
  "name": "smart-snake",
  "version": "0.0.0",
  "description": "Snake játék megvalósítása útvonalkereséssel",
  "main": "./src/js/main.js",
  "scripts": {
    "make": "npm run bundleapp; npm run bundlesimulator; npm run test",
    "simulate": "node ./dist/simulate.bundle.js",
    "bundlesimulator": "rollup --config rollup.simulator.config.js",
    "bundleapp": "rollup --config rollup.config.js",
    "serve": "RUN_ENV=browser nodemon ./server.js --config nodemon.json",
    "server": "RUN_ENV=browser node ./server.js --config nodemon.json",
    "test": "RUN_ENV=test npx mocha --require babel-core/register ./src/js/tests/**/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luminted/Szakdolgozat-SmartSnake.git"
  },
  "keywords": [
    "snake",
    "AI",
    "pathfinding"
  ],
  "author": "Levente Rozsenich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Luminted/Szakdolgozat-SmartSnake/issues"
  },
  "homepage": "https://github.com/Luminted/Szakdolgozat-SmartSnake#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.7.0",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.30.1",
    "mainloop.js": "^1.0.4",
    "mocha": "^6.1.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "uuid": "^3.3.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.16.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "sinon": "^7.3.2"
  }
}
